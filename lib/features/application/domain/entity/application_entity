import 'package:equatable/equatable.dart';

class ApplicationEntity extends Equatable {
  final String? applicationId;
  final String jobId;
  final String applicantId;
  final String status;

  const ApplicationEntity({
    this.applicationId,
    required this.jobId,
    required this.applicantId,
    this.status = 'pending',
  });

  @override
  List<Object?> get props => [
        applicationId,
        jobId,
        applicantId,
        status,
      ];
}